{
  "automerge": true,
  "rangeStrategy": "replace",
  "extends": [
    "config:recommended",
    "group:allNonMajor",
    ":preserveSemverRanges"
  ],
  "packageRules": [
    {
      "matchUpdateTypes": ["minor", "patch"],
      "matchPackageNames": ["@iconify/json"],
      "automerge": true,
      "minimumReleaseAge": "1 day",
      "extends": ["schedule:daily"]
    },
    {
      "matchUpdateTypes": ["minor", "patch"],
      "matchPackageNames": "^(?!@iconify/json).*$",
      "extends": ["schedule:weekly"]
    }
  ]
}
