{
  "automerge": false,
  "rangeStrategy": "pin",
  "extends": [
    "config:recommended",
    "group:allNonMajor",
    ":preserveSemverRanges"
  ],
  "packageRules": [
    {
      "matchUpdateTypes": ["pin"],
      "matchPackagePatterns": ["^@iconify/json"],
      "extends": ["schedule:daily"]
    },
    {
      "matchUpdateTypes": ["minor", "patch"],
      "excludePackagePatterns": ["^@iconify/json"],
      "extends": ["schedule:weekly"]
    }
  ]
}
